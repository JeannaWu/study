<% if logged_in? %>
<% @posts.each do |post| %>  
<body>
   <h5><%= link_to post.title, post%></h5>  
  
  <h3>     Posted at<i>-<%= time_ago_in_words post.created_at %> ago </i> ago.
       <%= post.user%></h3>
  </body>  
   
<% end %>
    <%=link_to "New Post", newpost_path%> 

  
         
       

<% else %>
      <div class="center jumbotron">
        <h1>Welcome to the Forum App</h1>
        <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
    </div>
        <%= link_to image_tag("home.png", alt: "...") %>
<% end %>