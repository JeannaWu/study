<% if logged_in? %>
<% @posts.each do |post| %>  
<body>
   <h5><%= link_to post.title, post%></h5>  
  
  <h3>     Posted at<i>-<%= time_ago_in_words post.created_at %> ago </i> ago.
       <%= post.user%></h3>
  </body>  
   
<% end %>
    <%=link_to "New Post", newpost_path%> 

  
         
       

